<?xml version="1.0"?>
<package >
  <metadata>
    <id>SassBuilder</id>
    <version>0.1.3</version>
    <title>SassBuilder</title>
    <summary>MSBuild extension. Automatically build **/*.scss to *.css</summary>
    <authors>JustForFun.click</authors>
    <owners>JustForFun.click</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>MSBuild extension. Automatically build **/*.scss to *.css during project build.</description>
    <tags>Sass Scss</tags>
    <projectUrl>https://github.com/justforfun-click/SassBuilder</projectUrl>
    <repository type="git" url="https://github.com/justforfun-click/SassBuilder.git"></repository>
    <license type="expression">MIT</license>
    <icon>sass.png</icon>
  </metadata>
  <files>
    <file src="LICENSE.md" target="LICENSE.md"/>
    <file src="README.md" target="README.md"/>
    <file src="CHANGELOG.md" target="CHANGELOG.md"/>
    <file src="sass.png" target="sass.png"/>
    <file src="build\SassBuilder.targets" target="build"/> 
    <file src="src\bin\Release\net5.0\publish\*.dll" target="build"/>
    <file src="src\bin\Release\net5.0\publish\*.json" target="build"/>
    <file src="src\bin\Release\net5.0\publish\runtimes\**" target="build/runtimes"/>
  </files>
</package>