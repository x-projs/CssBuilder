<?xml version="1.0"?>
<package >
  <metadata>
    <id>CssBuilder</id>
    <version>0.1.8-preview.2</version>
    <title>CssBuilder</title>
    <summary>MSBuild extension. Automatically build *.less/*.sass/*.scss to *.css</summary>
    <authors>JustForFun.click</authors>
    <owners>JustForFun.click</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>MSBuild extension. Automatically build *.less/*.sass/*.scss to *.css during project build.</description>
    <tags>Less Sass Scss</tags>
    <projectUrl>https://github.com/justforfun-click/CssBuilder</projectUrl>
    <repository type="git" url="https://github.com/justforfun-click/CssBuilder.git"></repository>
    <license type="expression">MIT</license>
    <icon>css.png</icon>
  </metadata>
  <files>
    <file src="LICENSE.md" target="LICENSE.md"/>
    <file src="README.md" target="README.md"/>
    <file src="CHANGELOG.md" target="CHANGELOG.md"/>
    <file src="css.png" target="css.png"/>
    <file src="build\CssBuilder.targets" target="build"/> 
    <file src="src\bin\Release\net5.0\publish\*.dll" target="build"/>
    <file src="src\bin\Release\net5.0\publish\*.json" target="build"/>
    <file src="src\bin\Release\net5.0\publish\runtimes\**" target="build/runtimes"/>
  </files>
</package>